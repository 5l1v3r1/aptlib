<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <TextView
                android:id="@+id/text1"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_margin="10dp"
                android:background="@drawable/textview_border"
                android:gravity="center"
                android:text="\@Async的使用" />

            <TextView
                android:id="@+id/text2"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_margin="10dp"
                android:background="@drawable/textview_border"
                android:gravity="center"
                android:text="\@Cacheable的使用" />

            <TextView
                android:id="@+id/text3"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_margin="10dp"
                android:background="@drawable/textview_border"
                android:gravity="center"
                android:onClick="clickText3"
                android:text="\@LogMethod的使用" />

            <TextView
                android:id="@+id/text4"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_margin="10dp"
                android:background="@drawable/textview_border"
                android:gravity="center"
                android:text="\@HookMethod的使用" />

            <TextView
                android:id="@+id/text5"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_margin="10dp"
                android:background="@drawable/textview_border"
                android:gravity="center"
                android:text="\@Prefs的使用" />

            <TextView
                android:id="@+id/text6"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_margin="10dp"
                android:background="@drawable/textview_border"
                android:gravity="center"
                android:text="\@Safe的使用" />

            <TextView
                android:id="@+id/text7"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_margin="10dp"
                android:background="@drawable/textview_border"
                android:gravity="center"
                android:text="\@Trace的使用" />

            <TextView
                android:id="@+id/text8"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_margin="10dp"
                android:background="@drawable/textview_border"
                android:gravity="center"
                android:text="\@Permission的使用" />

            <TextView
                android:id="@+id/text9"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_margin="10dp"
                android:background="@drawable/textview_border"
                android:gravity="center"
                android:onClick="clickText9"
                android:text="testactivity" />

        </LinearLayout>
    </ScrollView>


</LinearLayout>